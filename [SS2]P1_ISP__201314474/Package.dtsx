                            SqlCommand myCommand1 = new SqlCommand(query, myADONETConnection);
                            myCommand1.ExecuteNonQuery();
                        }
                        counter++;
                    }
                    SourceFile.Close();
                    Dts.TaskResult = (int)ScriptResults.Success;
                    //ROMEOPC\INSTANCIAROMEO
                    MessageBox.Show("Se insertaron " + (counter - 1) + " Elementos en la tabla Temporal VENTAS", "SE INFORMA:", MessageBoxButtons.OK, MessageBoxIcon.Exclamation);

